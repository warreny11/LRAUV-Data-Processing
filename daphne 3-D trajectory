fid = fopen('measured_parameter-3daphne.csv');
for i = 1:1000000
tline = fgetl(fid);
 
idx_10 = find(tline=='(',1);
daplon = tline(idx_10+1 : idx_10+21);
daplonnum(i) = str2num(daplon);
 
idx_11 = find(tline==')',1);
daplat = tline(idx_11-19 : idx_11-1);
daplatnum(i) = str2num(daplat);

idx_17 = find(tline==',',3);
idx_18 = find(tline==',',4);
dapdep = tline(idx_17(3)+1 : idx_18(4)-1);
dapdepnum(i) = str2num(dapdep);
 
end
 
plot3(daplatnum,daplonnum,-dapdepnum)
