fid = fopen('measured_parameter-3daphne.csv');
for i = 1:1000000
tline = fgetl(fid);
 
idx_10 = find(tline=='(',1);
daplonstr = tline(idx_10+1 : idx_10+21);
daplon(i) = str2num(daplonstr);
 
idx_11 = find(tline==')',1);
daplatstr = tline(idx_11-19 : idx_11-1);
daplat(i) = str2num(daplatstr);

idx_17 = find(tline==',',3);
idx_18 = find(tline==',',4);
dapdepstr = tline(idx_17(3)+1 : idx_18(4)-1);
dapdep(i) = str2num(dapdepstr);
 
end
 
plot3(daplat,daplon,-dapdep)
